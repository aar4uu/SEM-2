#swapping the no 

num1= int(input("Enter a number 1:"))
num2= int(input("Enter a number 2:"))

print("VALUES BEFORE SWAPPING:")
print("VALUE OF 1ST NO:", num1)
print("VALUE OF 2nd NO:", num2)

num1^=num2
num2^=num1
num1^=num2

print("VALUES AFTER SWAPPING:")
print("VALUE OF 1ST NO:", num1)
print("VALUE OF 2nd NO:", num2)


